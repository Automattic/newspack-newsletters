  "content": "<!-- wp:group {\"newsletterVisibility\":\"email\",\"templateLock\":\"all\",\"align\":\"full\",\"style\":{\"color\":{\"background\":\"__BG_COLOR__\",\"text\":\"__TEXT_COLOR__\"}}} -->\n<div class=\"wp-block-group alignfull has-text-color has-background\" style=\"background-color:__BG_COLOR__;color:__TEXT_COLOR__;\">\n\n<!-- wp:columns {\"verticalAlignment\":\"center\"} -->\n<div class=\"wp-block-columns are-vertically-aligned-center\"><!-- wp:column {\"templateLock\":\"all\",\"verticalAlignment\":\"center\",\"width\":\"66.66%\"} -->\n<div class=\"wp-block-column is-vertically-aligned-center\" style=\"flex-basis:66.66%\">__LOGO_OR_SITENAME__</div>\n<!-- /wp:column -->\n\n<!-- wp:column {\"templateLock\":\"all\",\"verticalAlignment\":\"center\",\"width\":\"33.33%\"} -->\n<div class=\"wp-block-column is-vertically-aligned-center\" style=\"flex-basis:33.33%\">__DATE_RIGHT__</div>\n<!-- /wp:column --></div>\n<!-- /wp:columns -->\n\n</div>\n<!-- /wp:group -->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:paragraph {\"fontSize\":\"huge\"} -->\n<p class=\"has-huge-font-size\">Start your newsletter with an intro, sometimes known as a “topper”.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:newspack-newsletters/posts-inserter {\"postsToShow\":1,\"excerptLength\":35,\"innerBlocksToInsert\":[],\"featuredImageAlignment\":\"top\"} /-->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:separator {\"color\":\"light-gray\",\"className\":\"is-style-wide\"} -->\n<hr class=\"wp-block-separator has-text-color has-background has-light-gray-background-color has-light-gray-color is-style-wide\"/>\n<!-- /wp:separator -->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:newspack-newsletters\/posts-inserter {\"postsToShow\":1,\"excerptLength\":35,\"innerBlocksToInsert\":[],\"featuredImageAlignment\":\"top\"} \/-->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:group {\"align\":\"full\",\"style\":{\"spacing\":{\"padding\":{\"top\":\"40px\",\"right\":\"0px\",\"bottom\":\"40px\",\"left\":\"0px\"}}},\"backgroundColor\":\"primary\",\"textColor\":\"white\"} -->\n<div class=\"wp-block-group alignfull has-white-color has-primary-background-color has-text-color has-background\" style=\"padding-top:40px;padding-right:0px;padding-bottom:40px;padding-left:0px\">\n\n<!-- wp:heading {\"level\":3} -->\n<h3 id=\"support-our-newsroom\">Support our newsroom</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph -->\n<p>Make sure you have a call-to-action to support your work. This can be a few sentences like this, reminding readers of the value of your newsroom, or it can be an image. Either way, make sure you give a clear CTA and a unique link to track support back to this email.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:buttons {\"layout\":{\"type\":\"flex\"}} -->\n<div class=\"wp-block-buttons\"><!-- wp:button {\"backgroundColor\":\"white\",\"textColor\":\"primary\",\"className\":\"is-style-fill\"} -->\n<div class=\"wp-block-button is-style-fill\"><a class=\"wp-block-button__link has-primary-color has-white-background-color has-text-color has-background\">Support us</a></div>\n<!-- /wp:button --></div>\n<!-- /wp:buttons -->\n\n</div>\n<!-- /wp:group -->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:social-links {\"layout\":{\"type\":\"flex\",\"justifyContent\":\"center\"}} -->\n<ul class=\"wp-block-social-links\"><!-- wp:social-link {\"url\":\"#\",\"service\":\"facebook\"} /-->\n\n<!-- wp:social-link {\"url\":\"#\",\"service\":\"twitter\"} /-->\n\n<!-- wp:social-link {\"url\":\"#\",\"service\":\"instagram\"} /-->\n\n<!-- wp:social-link {\"url\":\"#\",\"service\":\"youtube\"} /--></ul>\n<!-- /wp:social-links -->"
}
