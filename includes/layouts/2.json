  "content": "<!-- wp:group {\"newsletterVisibility\":\"email\",\"templateLock\":\"insert\",\"align\":\"full\",\"style\":{\"color\":{\"background\":\"__BG_COLOR__\",\"text\":\"__TEXT_COLOR__\"},\"spacing\":{\"padding\":{\"top\":\"40px\",\"right\":\"0px\",\"bottom\":\"40px\",\"left\":\"0px\"}}}} -->\n<div class=\"wp-block-group alignfull has-text-color has-background\" style=\"background-color:__BG_COLOR__;color:__TEXT_COLOR__;padding-top:40px;padding-right:0px;padding-bottom:40px;padding-left:0px\">\n\n__LOGO_OR_SITENAME_CENTER__\n\n__DATE_CENTER__\n\n</div>\n<!-- /wp:group -->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"textAlign\":\"center\",\"level\":3} -->\n<h3 class=\"has-text-align-center\">This would be the title</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"align\":\"center\",\"fontSize\":\"normal\"} -->\n<p class=\"has-text-align-center has-normal-font-size\">Start your newsletter with an intro, sometimes known as a “topper”. This is where the lead writer or editor for the newsletter introduces the main storylines for the day.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:separator {\"color\":\"light-gray\"} -->\n<hr class=\"wp-block-separator has-text-color has-background has-light-gray-background-color has-light-gray-color\"/>\n<!-- /wp:separator -->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:newspack-newsletters/posts-inserter {\"postsToShow\":1,\"innerBlocksToInsert\":[],\"featuredImageSize\":\"small\"} /-->\n\n<!-- wp:newspack-newsletters/posts-inserter {\"postsToShow\":1,\"innerBlocksToInsert\":[],\"featuredImageAlignment\":\"right\",\"featuredImageSize\":\"small\"} /-->\n\n<!-- wp:spacer {\"newsletterVisibility\":\"email\",\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:separator {\"newsletterVisibility\":\"email\",\"color\":\"light-gray\"} -->\n<hr class=\"wp-block-separator has-text-color has-background has-light-gray-background-color has-light-gray-color\"/>\n<!-- /wp:separator -->\n\n<!-- wp:spacer {\"newsletterVisibility\":\"email\",\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:heading {\"newsletterVisibility\":\"email\",\"textAlign\":\"center\",\"level\":3} -->\n<h3 class=\"has-text-align-center\">Support our newsroom</h3>\n<!-- /wp:heading -->\n\n<!-- wp:paragraph {\"newsletterVisibility\":\"email\",\"align\":\"center\"} -->\n<p class=\"has-text-align-center\">Make sure you have a call-to-action to support your work. This can be a few sentences like this, reminding readers of the value of your newsroom, or it can be an image. Either way, make sure you give a clear CTA and a unique link to track support back to this email.</p>\n<!-- /wp:paragraph -->\n\n<!-- wp:buttons {\"newsletterVisibility\":\"email\"} -->\n<div class=\"wp-block-buttons\"><!-- wp:button -->\n<div class=\"wp-block-button\"><a class=\"wp-block-button__link\">Support us</a></div>\n<!-- /wp:button --></div>\n<!-- /wp:buttons -->\n\n<!-- wp:spacer {\"newsletterVisibility\":\"email\",\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:group {\"newsletterVisibility\":\"email\",\"align\":\"full\",\"style\":{\"color\":{\"background\":\"#fafafa\"}}} -->\n<div class=\"wp-block-group alignfull has-background\" style=\"background-color:#fafafa\"><!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer -->\n\n<!-- wp:social-links {\"align\":\"center\",\"layout\":{\"type\":\"flex\"}} -->\n<ul class=\"wp-block-social-links aligncenter\"><!-- wp:social-link {\"url\":\"#\",\"service\":\"facebook\"} /-->\n\n<!-- wp:social-link {\"url\":\"#\",\"service\":\"twitter\"} /-->\n\n<!-- wp:social-link {\"url\":\"#\",\"service\":\"instagram\"} /-->\n\n<!-- wp:social-link {\"url\":\"#\",\"service\":\"youtube\"} /--></ul>\n<!-- /wp:social-links -->\n\n<!-- wp:spacer {\"height\":\"16px\"} -->\n<div style=\"height:16px\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div>\n<!-- /wp:spacer --></div>\n<!-- /wp:group -->"
}
